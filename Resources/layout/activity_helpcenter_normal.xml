<LocationConnection.HelpCenterConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
	android:background="@android:color/white"
    android:id="@+id/MainLayout">
	<ImageButton
		android:layout_width="35dp"
    	android:layout_height="35dp"
		app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/OpenTutorial"
		android:layout_marginTop="10dp"
		android:layout_marginLeft="10dp"
		android:tooltipText="@string/Back"
		android:background="?attr/selectableItemBackgroundBorderless"
		android:src="@drawable/ic_back_normal"
		android:id="@+id/HelpCenterBack" />
	<TextView
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		app:layout_constraintTop_toTopOf="@+id/HelpCenterBack"
		app:layout_constraintBottom_toBottomOf="@+id/HelpCenterBack"
		app:layout_constraintRight_toRightOf="parent"
		android:layout_marginRight="20dp"
		android:theme="@style/BlackText20Normal"
		android:text="@string/MenuHelpCenter"
		android:id="@+id/HelpCenterHeaderText" />   
    <Button
		android:layout_width="0px"
		android:layout_height="45dp"
		app:layout_constraintTop_toBottomOf="@+id/HelpCenterBack"
		app:layout_constraintBottom_toTopOf="@+id/QuestionsScroll"
		app:layout_constraintLeft_toLeftOf="parent"
		app:layout_constraintRight_toRightOf="parent"	
		android:layout_marginTop="10dp"	
		android:layout_marginBottom="10dp"	
		android:layout_marginLeft="10dp"
		android:layout_marginRight="10dp"
        android:textAllCaps="false"
        android:backgroundTint="@color/ButtonOnWhite"
        android:theme="@style/ButtonNormal"
        android:text="@string/HelpCenterTutorial"
		android:id="@+id/OpenTutorial" />    
	<ScrollView
		android:layout_width="match_parent"
		android:layout_height="0px"
		app:layout_constraintTop_toBottomOf="@+id/OpenTutorial"
		app:layout_constraintBottom_toTopOf="@+id/HelpCenterFormCaption"
        android:fadeScrollbars="false"
        android:id="@+id/QuestionsScroll">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
            android:paddingTop="10dp" 
            android:paddingBottom="10dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
			android:id="@+id/QuestionsContainer" />
	</ScrollView>
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        app:layout_constraintTop_toTopOf="@+id/QuestionsScroll"
        android:background="@drawable/separator_top_transparent" /> 
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/QuestionsScroll"
        android:background="@drawable/separator_bottom_transparent" /> 
	<TextView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		app:layout_constraintTop_toBottomOf="@+id/QuestionsScroll"
		app:layout_constraintBottom_toTopOf="@+id/MessageEdit"
		android:text="@string/HelpCenterFormCaption"
		android:theme="@style/BlackTextMediumNormal"
		android:textStyle="bold"		
		android:padding="10dp"
        android:background="@color/ContentBackground"
		android:id="@+id/HelpCenterFormCaption" />
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        app:layout_constraintTop_toBottomOf="@+id/HelpCenterFormCaption"
        android:background="@drawable/separator_top_transparent" /> 
	<EditText
		android:layout_width="match_parent"
		android:layout_height="0px"
		app:layout_constraintTop_toBottomOf="@+id/HelpCenterFormCaption"
		app:layout_constraintBottom_toTopOf="@+id/MessageSend"
		android:layout_marginLeft="10dp"
		android:layout_marginRight="10dp"
		android:layout_marginTop="10dp"		
		android:gravity="top"
        style="@android:style/Widget.EditText"	
        android:theme="@style/EditNormal"		
		android:id="@+id/MessageEdit" />
	<Button
		android:layout_width="wrap_content"
		android:layout_height="45dp"
		app:layout_constraintTop_toBottomOf="@+id/MessageEdit"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintLeft_toLeftOf="parent"
		android:layout_marginLeft="10dp"
		android:layout_marginBottom="10dp"
        android:textAllCaps="false"
        android:backgroundTint="@color/ButtonOnWhite"
        android:theme="@style/ButtonNormal"
        android:text="@string/HelpCenterFormSend"
		android:id="@+id/MessageSend" />	
	
	<androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
		android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
		app:layout_constraintBottom_toTopOf="@+id/TutorialFrame"
		android:padding="10dp"
        android:background="@android:color/white"
        android:visibility="invisible"
		android:clickable="true"
        android:id="@+id/TutorialTopBar">
		<ImageButton
			android:layout_width="35dp"
			android:layout_height="35dp"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"			
			android:tooltipText="@string/Back"
			android:background="?attr/selectableItemBackgroundBorderless"
			android:src="@drawable/ic_back_normal"
			android:id="@+id/TutorialBack" />
		<TextView
			android:layout_width="0px"
			android:layout_height="wrap_content"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"	
			app:layout_constraintRight_toRightOf="parent"				
			android:layout_marginLeft="55dp"
			android:text=""
			android:theme="@style/BlackTextSmallNormal"
			android:id="@+id/TutorialText" />
	</androidx.constraintlayout.widget.ConstraintLayout>
	<View
        android:layout_width="0px"
        android:layout_height="0px"
        app:layout_constraintTop_toTopOf="@+id/TutorialFrame"
		app:layout_constraintBottom_toBottomOf="@+id/TutorialFrame"
		app:layout_constraintLeft_toLeftOf="@+id/TutorialFrame"
		app:layout_constraintRight_toRightOf="@+id/TutorialFrame"
        android:background="@color/ContentDarkBackground"
		android:visibility="invisible"
		android:id="@+id/TutorialFrameBg" />
	<ImageView
		android:layout_width="36dp"
		android:layout_height="36dp"
		app:layout_constraintTop_toTopOf="@+id/TutorialFrame"
		app:layout_constraintBottom_toBottomOf="@+id/TutorialFrame"
		app:layout_constraintLeft_toLeftOf="@+id/TutorialFrame"
		app:layout_constraintRight_toRightOf="@+id/TutorialFrame"
		android:src="@drawable/ic_reload36_black_4x"
		android:visibility="invisible"
		android:id="@+id/LoaderCircle" />    
	<LocationConnection.TouchConstraintLayout
    	android:layout_width="match_parent"
    	android:layout_height="0px"
        app:layout_constraintTop_toBottomOf="@+id/TutorialTopBar"
		app:layout_constraintBottom_toTopOf="@+id/TutorialNavBar"
		android:visibility="invisible"               
        android:clickable="true"
    	android:id="@+id/TutorialFrame" />
	<View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        app:layout_constraintTop_toTopOf="@+id/TutorialFrame"
        android:background="@drawable/separator_top_transparent"
		android:visibility="invisible"
		android:id="@+id/TutorialTopSeparator" /> 
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/TutorialFrame"
        android:background="@drawable/separator_bottom_transparent"
		android:visibility="invisible"
		android:id="@+id/TutorialBottomSeparator" />	       
	<androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
		android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/TutorialFrame"
		app:layout_constraintBottom_toBottomOf="parent"
        android:background="@android:color/white"
        android:visibility="invisible"
		android:clickable="true"
        android:id="@+id/TutorialNavBar">
		<ImageButton
            android:layout_width="0px"
    		android:layout_height="45dp"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toLeftOf="parent"
			app:layout_constraintRight_toLeftOf="@+id/TutorialNavText"
			android:layout_marginRight="10dp"
    		android:src="@drawable/ic_previous_list_45x45_normal"
    		android:tooltipText="@string/LoadPrevious"
            android:background="?attr/selectableItemBackgroundBorderless"
            android:id="@+id/LoadPrevious" /> 
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
			app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/LoadPrevious"
            app:layout_constraintRight_toLeftOf="@+id/LoadNext"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
			android:gravity="center_horizontal"
            android:text=""
			android:theme="@style/BlackTextSmallNormal"
            android:id="@+id/TutorialNavText" />  
        <ImageButton
            android:layout_width="0px"
    		android:layout_height="45dp"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintLeft_toRightOf="@+id/TutorialNavText"
			app:layout_constraintRight_toRightOf="parent"
			android:layout_marginLeft="10dp"
    		android:src="@drawable/ic_previous_list_45x45_normal"
            android:scaleX="-1"
    		android:tooltipText="@string/LoadNext"
            android:background="?attr/selectableItemBackgroundBorderless"
            android:id="@+id/LoadNext" /> 
    </androidx.constraintlayout.widget.ConstraintLayout>
</LocationConnection.HelpCenterConstraintLayout>